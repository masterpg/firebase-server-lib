    "json-server": "^0.14.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.3",
    "postcss-cli": "^6.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-each": "^0.10.0",
    "postcss-extend": "^1.0.5",
    "postcss-for": "^2.1.1",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-media-variables": "^2.0.1",
    "postcss-mixins": "^6.2.0",
    "postcss-nesting": "^6.0.0",
    "prettier": "^1.14.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.1",
    "stylus": "^0.54.5",
