{
  "name": "vue-base-project",
  "scripts": {
    "dev": "node json-server.js & vue-cli-service serve --mode development",
    "stg": "vue-cli-service build --mode staging || npm run serve",
    "prod": "vue-cli-service build --mode production || npm run serve",
    "build": "vue-cli-service build --mode production",
    "build:dev": "vue-cli-service build --mode development",
    "build:stg": "vue-cli-service build --mode staging",
    "build:styles": "postcss --config build/postcss.config.js src/index.pcss -o dist/index.css",
    "serve": "node json-server.js & browser-sync start --port 5000 --proxy '0.0.0.0:5010' --serveStatic 'dist' --no-open",
    "test:unit": "vue-cli-service test:unit",
    "docs": "postcss --config build/postcss.config.js docs/book.pcss -o docs/book.css && cd docs && ../node_modules/.bin/gitbook install && ../node_modules/.bin/gitbook serve",
    "lint": "./scripts/lint.sh"
  },
  "dependencies": {
    "@quasar/extras": "^1.1.2",
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "animate.css": "^3.7.0",
    "axios": "^0.18.0",
    "lodash": "^4.17.11",
    "quasar": "^1.0.0-beta.24",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-i18n": "^8.11.2",
    "vue-property-decorator": "^8.1.1",
    "vue-router": "^3.0.6"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.129",
    "@types/node": "^12.0.2",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-plugin-pwa": "^3.7.0",
    "@vue/cli-plugin-typescript": "^3.7.0",
    "@vue/cli-plugin-unit-jest": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/eslint-config-typescript": "^4.0.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-imports": "^1.5.1",
    "browser-sync": "^2.26.5",
    "copy-webpack-plugin": "^5.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.2.2",
    "gitbook-cli": "^2.3.2",
    "json-server": "^0.14.2",
    "postcss-cli": "^6.1.2",
    "postcss-each": "^0.10.0",
    "postcss-extend": "^1.0.5",
    "postcss-for": "^2.1.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.1",
    "postcss-nested": "^4.1.2",
    "postcss-rtl": "^1.3.3",
    "prettier": "^1.17.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "testdouble": "^3.11.0",
    "testdouble-jest": "^2.0.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5",
    "vue-cli-plugin-quasar": "^1.0.0-beta.1",
    "vue-template-compiler": "^2.6.10",
    "yaml-loader": "^0.5.0"
  }
}
