{
  "name": "firebase-server-lib-functions",
  "private": true,
  "engines": {
    "node": "12"
  },
  "main": "dist/app/index.js",
  "scripts": {
    "build": "rm -rf dist && npm run gql:build && tsc --project ./tsconfig.build.json",
    "serve": "npm run build && export GOOGLE_APPLICATION_CREDENTIALS=../serviceAccountKey.json && (npm run gql:watch & tsc --project ./tsconfig.build.json --watch & firebase serve --only functions --port=5010)",
    "shell": "npm run build && export GOOGLE_APPLICATION_CREDENTIALS=../serviceAccountKey.json && (npm run gql:watch & tsc --project ./tsconfig.build.json --watch & firebase functions:shell --port=5010)",
    "test": "export GOOGLE_APPLICATION_CREDENTIALS=../serviceAccountKey.json && jest --runInBand",
    "test:app": "export GOOGLE_APPLICATION_CREDENTIALS=../serviceAccountKey.json && jest --runInBand /app/",
    "test:firestore-ex": "export GOOGLE_APPLICATION_CREDENTIALS=../serviceAccountKey.json && jest --runInBand /firestore-ex/",
    "test:pre": "npm run gql generate ./tests/mocks/app ./tests/mocks/app",
    "test:coverage": "npm run test -- --coverage",
    "lint": "./node_modules/web-base-lib/tools/lint.sh",
    "deploy": "export GOOGLE_APPLICATION_CREDENTIALS=../serviceAccountKey.json && firebase deploy --only functions,firestore",
    "firebase": "firebase emulators:start",
    "gql": "node ./tools/gql.js",
    "gql:build": "npm run gql build ./src ./src/app ./dist",
    "gql:watch": "npm run gql build ./src ./src/app ./dist -- -w",
    "user": "export GOOGLE_APPLICATION_CREDENTIALS=../serviceAccountKey.json && node ./dist/tools/user.js",
    "storage": "export GOOGLE_APPLICATION_CREDENTIALS=../serviceAccountKey.json && node ./dist/tools/storage.js",
    "keepalive": "export GOOGLE_APPLICATION_CREDENTIALS=../serviceAccountKey.json && node ./dist/tools/keepalive.js"
  },
  "dependencies": {
    "@google-cloud/logging": "^8.1.1",
    "@nestjs/common": "7.0.7",
    "@nestjs/core": "7.0.7",
    "@nestjs/graphql": "7.1.5",
    "@nestjs/platform-express": "7.0.7",
    "apollo-server-express": "2.11.0",
    "chokidar": "^3.3.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "convert-hrtime": "^3.0.0",
    "dayjs": "^1.8.23",
    "firebase-admin": "^9.4.1",
    "firebase-functions": "^3.11.0",
    "firebase-tools": "^8.16.2",
    "glob": "^7.1.6",
    "graphql": "14.6.0",
    "graphql-subscriptions": "1.1.0",
    "graphql-toolkit": "0.7.5",
    "graphql-tools": "4.0.7",
    "graphql-type-json": "0.3.1",
    "lodash": "^4.17.15",
    "on-finished": "^2.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "vary": "^1.1.2",
    "web-base-lib": "https://github.com/masterpg/web-base-lib.git"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.0.7",
    "@types/graphql-type-json": "^0.3.2",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^25.1.5",
    "@types/lodash": "^4.14.150",
    "@types/on-finished": "^2.3.1",
    "@types/supertest": "^2.0.8",
    "@types/vary": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "chalk": "^4.1.0",
    "commander": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "inquirer": "^7.2.0",
    "jest": "^25.2.7",
    "prettier": "^2.0.2",
    "supertest": "^4.0.2",
    "testdouble": "^3.13.1",
    "testdouble-jest": "^2.0.0",
    "ts-jest": "^25.3.0",
    "typescript": "^3.8.3"
  }
}
