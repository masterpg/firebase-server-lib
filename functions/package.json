{
  "name": "functions",
  "scripts": {
    "lint": "cd .. && ./scripts/lint.sh functions",
    "build": "tsc && cp ./src/serviceAccountKey.json ./lib",
    "serve": "npm run build && ./functions.config.dev.sh && firebase functions:config:get > .runtimeconfig.json && (tsc --watch & firebase serve --only functions --port=5010)",
    "shell": "npm run build && ./functions.config.dev.sh && firebase functions:config:get > .runtimeconfig.json && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "./functions.config.prod.sh && firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "10"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@google-cloud/logging": "^5.2.1",
    "@types/vary": "^1.1.0",
    "apollo-server-express": "^2.8.1",
    "class-transformer": "^0.2.3",
    "convert-hrtime": "^3.0.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "firebase-admin": "^8.3.0",
    "firebase-functions": "^3.2.0",
    "firebase-tools": "^7.2.2",
    "graphql": "^14.4.2",
    "graphql-type-json": "^0.3.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^3.3.0",
    "type-graphql": "^0.17.4",
    "vary": "^1.1.2"
  },
  "devDependencies": {
    "@types/graphql-type-json": "^0.3.0",
    "typescript": "^3.5.3"
  },
  "private": true
}
